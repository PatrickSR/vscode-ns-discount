{
	"name": "ns-discount",
	"displayName": "ns-discount",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"activationEvents": [
		"onView:featured",
		"onView:subscribe",
		"onCommand:nsDiscount.subscribe.add"
	],
	"contributes": {
		"commands": [{
			"command": "nsDiscount.featured.more",
			"title": "查看更多折扣"
		}, {
			"command": "nsDiscount.subscribe.add",
			"title": "添加关注的游戏"
		}],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "ns-discount",
					"title": "NS Discount",
					"icon": "media/ns.svg"
				}
			]
		},
		"views": {
			"ns-discount": [
				{
					"id": "featured",
					"name": "折扣推荐"
				},
				{
					"id": "subscribe",
					"name": "我的订阅"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"axios": "^0.20.0"
	}
}
